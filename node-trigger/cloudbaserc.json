{
  "version": "2.0",
  "envId": "{{envId}}",
  "$schema": "https://framework-1258016615.tcloudbaseapp.com/schema/latest.json",
  "functionRoot": "./functions",
  "functions": [
    {
      "name": "node-trigger",
      "timeout": 5,
      "runtime": "Nodejs10.15",
      "memorySize": 128,
      "triggers": [
        { "name": "trigger", "type": "timer", "config": "0 0 2 1 * * *" }
      ],
      "handler": "index.main"
    }
  ],
  "framework": {
    "name": "node-trigger",
    "plugins": {
      "function": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {}
      }
    }
  }
}
