{
  "version": "2.0",
  "envId": "{{envId}}",
  "$schema": "https://framework-1258016615.tcloudbaseapp.com/schema/latest.json",
  "functionRoot": "./functions",
  "functions": [
    {
      "name": "java-app",
      "timeout": 5,
      "runtime": "Java8",
      "memorySize": 512,
      "params": {},
      "handler": "example.Hello::mainHandler"
    }
  ],
  "framework": {
    "name": "java-starter",
    "plugins": {
      "function": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {}
      }
    }
  }
}
